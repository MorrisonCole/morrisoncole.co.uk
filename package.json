{
  "private": true,
  "name": "monorepo",
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@types/css": "0.0.33",
    "@types/eslint": "8.4.1",
    "@types/node": "17.0.13",
    "@types/prettier": "2.4.3",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.2",
    "axios": "0.25.0",
    "css": "3.0.0",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "7.28.0",
    "prettier": "2.5.1",
    "ts-node": "10.4.0",
    "typescript": "4.5.5",
    "vercel": "23.1.2"
  },
  "scripts": {
    "start:gatsby": "yarn workspace morrisoncole.co.uk start",
    "start:next": "yarn workspace nextjs-site dev",
    "build:gatsby": "yarn workspace morrisoncole.co.uk build",
    "build:next": "yarn workspace nextjs-site build",
    "serve:gatsby": "yarn workspace morrisoncole.co.uk serve",
    "serve:next": "yarn workspace nextjs-site start",
    "deploy:gatsby": "yarn workspace morrisoncole.co.uk deploy",
    "test:next": "yarn workspace nextjs-site test:lighthouse",
    "upgrade": "yarn set version latest",
    "prettier:fix": "yarn prettier \"**/*.{tsx,ts,js,md,mdx,json,yml,html,css}\" --write",
    "get-fonts": "ts-node -T scripts/get-fonts.ts"
  },
  "packageManager": "yarn@3.1.1"
}
